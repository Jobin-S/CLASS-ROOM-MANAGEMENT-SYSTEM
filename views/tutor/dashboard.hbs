        <!-- Notice Board Start Here -->
        <div class="col-lg-6 col-4-xxxl col-xl-6">
            <div class="card dashboard-card-six">
                <div class="card-body">
                    <div class="heading-layout1 mg-b-17">
                        <div class="item-title">
                            <h3>Events</h3>
                        </div>
                        
                    </div>
                    <div class="notice-box-wrap">
                        {{#each events}}
                        <div class="notice-list">
                            <div class="post-date bg-skyblue">{{date}}</div>
                            <h3 class="notice-title"><a href="#">{{title}}</a></h3>
                            <h6 class="notice-title"><a href="#">{{description}}</a></h6>
                            {{#if isPaid}}
                            <div class="entry-meta">Paid Event /<span> Rs {{price}}</span></div>
                            {{/if}}
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
        <!-- Notice Board End Here -->
        <!-- Notice Board Start Here -->
        <div class="col-lg-6 col-4-xxxl col-xl-6">
            <div class="card dashboard-card-six">
                <div class="card-body">
                    <div class="heading-layout1 mg-b-17">
                        <div class="item-title">
                            <h3>announcements</h3>
                        </div>
                        
                    </div>
                    <div class="notice-box-wrap">
                        {{#each announcements}}
                        <div class="notice-list">
                            <div class="post-date bg-pink">{{date}}</div>
                            <h3 class="notice-title"><a href="/tutor/announcement/{{_id}}">{{title}}</a></h3>
                            <h6 class="notice-title"><a href="#">{{description}}</a></h6>
                            
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
        <!-- Notice Board End Here -->
        